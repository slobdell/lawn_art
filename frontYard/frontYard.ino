  for(int r=0; r<frameHeight; r++) {
    for(int c=0; c<frameWidth; c++) {
      int movedCol = (movingOffset + c) % frameWidth;
      int pixelIndex = r * frameWidth;
      pixelIndex += movedCol;
      leds->setPixel(
        pixelIndex,
        basicColor
      );
    }
  }
}

