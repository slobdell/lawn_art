void initializeArray(int *arr, int start_, int end_, bool reverse) {
  int cursorIndex = 0;
  //arr = new int[end_ - start_];
  if (reverse) {
    for(int i = end_; i >= 0; i--) {
      arr[cursorIndex] = i;
      cursorIndex++;
    } 
  } else {
    for(int i = start_; i < end_; i++) {
      arr[cursorIndex] = i;
      cursorIndex++;
    }  
  }  
}



